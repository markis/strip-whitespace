{
  "name": "strip-whitespace",
  "version": "0.0.10",
  "bin": ".bin/strip-whitespace",
  "description": "Strip whitespace in strings from javascript assets",
  "main": "index.js",
  "scripts": {
    "prepublish": "in-publish && npm test && npm run build || not-in-publish ",
    "lint": "tslint -c tslint.json ./src/**/*.ts",
    "test": "npm run lint && rollup -c -o tests.js -i tests/index.ts && node tests.js",
    "build": "npm run build:index && npm run build:cli && npm run build:declarations",
    "build:declarations": "tsc -d && mv tmp/src/index.d.ts ./index.d.ts",
    "build:index": "rollup -c -o index.js -i src/index.ts",
    "build:cli": "rollup -c -o cli.js -i src/cli.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/markis/strip-whitespace.git"
  },
  "keywords": [
    "whitespace",
    "strings",
    "strip-whitespace",
    "minification"
  ],
  "author": "Markis Taylor <m@rkis.cc>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markis/strip-whitespace/issues"
  },
  "homepage": "https://github.com/markis/strip-whitespace#readme",
  "dependencies": {
    "typescript": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.8",
    "in-publish": "^2.0.0",
    "rollup": "^0.41.5",
    "rollup-plugin-typescript": "^0.8.1",
    "tslint": "^5.0.0"
  }
}
